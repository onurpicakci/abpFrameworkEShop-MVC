@page
@using EShop.Web.Pages.Categories
@using EShop.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EShop.Web.Pages.Categories.EditModalModel
@inject IStringLocalizer<EShopResource> L
@{
    Layout = null;
}

<abp-dynamic-form abp-model="Category" asp-page="/Categories/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["Update"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id"></abp-input>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@AbpModalButtons.Cancel|AbpModalButtons.Save"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>

